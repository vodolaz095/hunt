{
  "name": "hunt",
  "version": "0.1.4",
  "description": "High level nodejs framework build on top of expressjs, mongoose, sequilize, socketio and passportjs",
  "main": "index.js",
  "engines": {
    "node": ">=0.10.26",
    "npm": ">=1.4.3"
  },
  "os" : [ "linux", "darwin" ],
  "scripts": {
    "start": "node examples/fullExample.js",
    "test": "./node_modules/.bin/mocha --reporter spec; ./generateDocs.sh",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:vodolaz095/hunt.git"
  },
  "directories": {
    "example":"./examples",
    "doc": "./examples/views/documentation",
    "lib": "./lib",
    "test":"./test"
  },
  "bugs": {
    "url": "https://bitbucket.org/vodolaz095/hunt/issues"
  },
  "keywords": [
    "hunt",
    "boilerplate",
    "framework",
    "express",
    "mongoose",
    "sequilize",
    "socket.io",
    "passportjs"
  ],
  "author": {
    "name": "Anatolij Ostroumov",
    "url": "https://github.com/vodolaz095",
    "email": "ostroumov095@gmail.com"
  },
  "homepage": "https://huntjs.herokuapp.com/",
  "license": "MIT",
  "dependencies": {
    "async": ">=0.9.0",
    "validator": ">=3.0.0",
    "express": ">4.0.0",
    "body-parser": ">=1.0.0",
    "connect-multiparty": ">=1.0.5",
    "compression":">=1.0.2",
    "serve-favicon":">=2.0.0",
    "cookie-parser":">=1.0.0",
    "method-override":">=1.0.0",
    "express-session":">=1.0.0",
    "response-time":">=1.0.0",
    "morgan":">=1.0.1",
    "errorhandler":">=1.0.1",
    "csurf":">=1.1.0",
    "express-resource": "~1.0.0",
    "hogan-express": "~0.5.0",
    "jade": ">=1.3.0",
    "swig": ">=1.2.2",
    "redis": ">=0.10.0",
    "connect-redis": ">=2.0.0",
    "connect-flash": "*",
    "socket.io": "0.9.17",
    "passport.socketio": ">=3.0.0",
    "passport": "*",
    "passport-local": "*",
    "passport-hash": "*",
    "passport-google": "*",
    "passport-github": "*",
    "passport-twitter": "*",
    "passport-facebook": "*",
    "passport-vkontakte": "*",
    "passport-yahoo": "*",
    "passport-intuit": "*",
    "passport-paypal": "*",
    "passport-steam": "*",
    "mongoose": "3.8.12",
    "sequelize": "*",
    "colors": "*",
    "nodemailer": "*",
    "rai":"<0.2.0"
  },
  "optionalDependencies": {
    "hiredis": "*"
  },
  "devDependencies": {
    "mocha": ">=1.20.0",
    "istanbul": "*",
    "should": "3.x",
    "jsdoc": "*",
    "request":"latest",
    "sqlite3":"*"
  }
}
